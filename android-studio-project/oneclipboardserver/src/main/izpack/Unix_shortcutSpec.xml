<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<shortcuts>
    <skipIfNotSupported />
    <programGroup defaultName="${APP_NAME}" location="applications" />
    <shortcut
        name="${APP_NAME}" target="$INSTALL_PATH\${UNIX_START_SCRIPT}" description="${APP_NAME}"
        iconFile="$INSTALL_PATH/logo.png" workingDirectory="$INSTALL_PATH" initialState="normal"
        programGroup="yes" desktop="yes" applications="no" startMenu="no" startup="yes" type="Application"
        encoding="UTF-8">
    </shortcut>
</shortcuts>
